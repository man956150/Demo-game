{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended",
        "prettier"
    ],
    "settings":{
        "react-native":{
            "version":"latest"
        }
    },
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "react-native"
    ],
    "rules": {
        "indent":["error",4],
        // "linebreak-style":["error","unix"],
        "semi":["off"],
        "quotes":["error","single"],
        "no-unused-vars":["off"],
        "no-use-before-define": ["error", { "variables": false }],
        "react/prop-types": ["error", {"ignore": ["navigation", "navigation.navigate"]}]
    }
}
